<!DOCTYPE html>
<html>
	<head>
		<title>Yoogle</title>
		<link href="stylesheets/layout.css" rel="stylesheet" type="text/css">
		<link href="stylesheets/jquery-ui.css" rel="stylesheet" type="text/css">
		<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
		<script src="javascripts/map.js" language="javascript" type="text/javascript"></script>
		<script src="javascripts/jquery.js" language="javascript" type="text/javascript"></script>
		<script src="javascripts/jquery-ui.js" language="javascript" type="text/javascript"></script>

		<style>
			.test1 {
background:#F00;
			}
			.test1:hover {
background:#0FF;
			}
		</style>
	</head>
	<body onload="initializeMap();">
		<div class="InputBody" id="input" style="boarder:10px solid #000000;">
			<input id="querytext" onKeyUp="keypress();">
		</div>
		<div class="ResultBody" id="result" style="overflow-y:auto;">
			<table id="query">
				<tr onmouseover="this.addClass('select');" onmouseout="this.removeClass('select');">
					<td id="showquery" >
					</td>
				</tr>
				<tr class="test1"> <!-- onmouseover="this.bgColor='#000000';" onmouseout="this.removeClass('select');"-->
					<td id="showqueryext">
					</td>
				</tr>
			</table>
		</div>
		<div class="MapBody" id="map_canvas"></div>
	</body>
</html>
